{
  "private": true,
  "name": "bare-expo",
  "description": "Example of embedding Bare in an Expo application",
  "main": "expo-router/entry",
  "scripts": {
    "bundle": "bare-pack --target ios --target android  --linked --out app/app.bundle.mjs backend/backend.mjs",
    "start": "expo start",
    "ios": "expo run:ios --device 'iPhone 16'",
    "ios2": "expo run:ios --device 'iPhone 15'",
    "android": "expo run:android"
  },
