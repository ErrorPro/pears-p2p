  "dependencies": {
    "@react-native-clipboard/clipboard": "^1.16.3",
    "b4a": "^1.7.3",
    "bare-fs": "^4.5.2",
    "bare-rpc": "^1.0.0",
    "expo": "^54.0.10",
    "expo-build-properties": "^1.0.9",
    "expo-constants": "^18.0.9",
    "expo-dev-client": "~6.0.18",
    "expo-file-system": "^19.0.19",
